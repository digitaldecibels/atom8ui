{
    "name": "pantheon-upstreams/drupal-composer-managed",
    "description": "Install Drupal 9 with Composer on Pantheon.",
    "type": "project",
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "path",
            "url": "upstream-configuration"
        },
        {
            "type": "package",
            "package": {
                "name": "enyo/dropzone",
                "version": "6.0.0-beta.2",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/dropzone/dropzone/releases/download/v6.0.0-beta.2/dist.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "exif-js/exif-js",
                "version": "v2.3.0",
                "type": "drupal-library",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/exif-js/exif-js/archive/refs/tags/v2.3.0.zip"
                }
            }
        }
    ],
    "require": {
        "composer/installers": "^2",
        "drupal/core-composer-scaffold": "^10",
        "drupal/core-recommended": "^10",
        "cweagans/composer-patches": "^1.7",
        "drush/drush": "^12.4",
        "drupal/admin_toolbar": "^3.0",
        "drupal/auto_entitylabel": "^3.0@beta",
        "drupal/backup_migrate": "^5.0@RC",
        "drupal/coffee": "^1.2",
        "drupal/core-project-message": "^9.1",
        "drupal/devel": "^5.1",
        "drupal/editor_advanced_link": "^2.0",
        "drupal/email_registration": "^2.0@RC",
        "drupal/entity_delete": "^2.0@beta",
        "drupal/entity_print": "2.x",
        "drupal/entity_usage": "^2.0@beta",
        "drupal/field_group": "^4.0",
        "drupal/flood_control": "^2.2",
        "drupal/hook_post_action": "^1.1",
        "drupal/inline_entity_form": "^3.0@RC",
        "drupal/masquerade": "^2.0@beta",
        "drupal/mimemail": "^1.0@alpha",
        "drupal/paragraphs": "^1.12",
        "drupal/queue_ui": "^3.1",
        "drupal/r4032login": "^2.1",
        "drupal/simple_mail": "^2.0",
        "drupal/twig_tweak": "^3.1",
        "drupal/views_bulk_operations": "^4.2",
        "drupal/views_data_export": "^1.0",
        "twilio/sdk": "*",
        "drupal/image_widget_crop": "^2.3",
        "drupal/image_resize_filter": "^1.2",
        "drupal/minifyhtml": "^2.0",
        "drupal/rest_export_nested": "^1.0",
        "drupal/config_ignore": "^3.3",
        "drupal/sendgrid_integration": "^2.2",
        "drupal/pathauto": "^1.11",
        "chillerlan/php-qrcode": "^5.0",
        "drupal/webform": "^6.2",
        "drupal/metatag": "^2.0",
        "drupal/entity_clone": "^2.0@beta",
        "drupal/xmlsitemap": "^1.5",
        "drupal/google_analytics": "^4.0",
        "drupal/editablefields": "^1.0",
        "drupal/recaptcha": "^3.2",
        "drupal/redirect": "^1.11",
        "drupal/better_exposed_filters": "^7.0",
        "drupal/eca": "^2.1",
        "drupal/bpmn_io": "^2.0",
        "drupal/barcodes": "^2.1",
        "drupal/taxonomy_import": "^2.0",
        "drupal/front": "^10.0@beta",
        "drupal/token_filter": "^2.2",
        "drupal/diff": "^2.0@beta",
        "drupal/address": "^2.0",
        "drupal/profile": "^1.12",
        "mpdf/mpdf": "*",
        "tecnickcom/tcpdf": "*",
        "setasign/fpdi": "*",
        "drupal/dropzonejs": "^2.11",
        "exif-js/exif-js": "*",
        "drupal/entity_browser": "^2.0",
        "enyo/dropzone": "*",
        "drupal/group": "^3.3"
    },
    "require-dev": {
        "drupal/core-dev": "^10"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "./web"
            },
            "allowed-packages": [
                "pantheon-systems/drupal-integrations"
            ],
            "file-mapping": {
                "[project-root]/.editorconfig": false,
                "[project-root]/pantheon.upstream.yml": false,
                "[project-root]/.gitattributes": false
            }
        },
        "enable-patching": true,
        "patches": {
            "drupal/commerce": {
                "Add commerce fees": "https://www.drupal.org/files/issues/2020-11-18/add-plugin-type-2903716-72.patch"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "web/private/scripts/quicksilver/{$name}/": [
                "type:quicksilver-script"
            ]
        },
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        }
    },
    "autoload": {
        "classmap": [
            "upstream-configuration/scripts/ComposerScripts.php"
        ]
    },
    "scripts": {
        "pre-update-cmd": [
            "DrupalComposerManaged\\ComposerScripts::preUpdate"
        ],
        "post-update-cmd": [
            "DrupalComposerManaged\\ComposerScripts::postUpdate"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": false,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "drupal/core-project-message": true,
            "endroid/installer": true,
            "drupal/core-project": true,
            "phpstan/extension-installer": true,
            "php-http/discovery": true,
            "tbachert/spi": true
        },
        "platform": {
            "php": "8.1.13"
        }
    }
}
