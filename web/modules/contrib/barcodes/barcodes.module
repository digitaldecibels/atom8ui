<?php

/**
 * @file
 * Legacy hook implementations for the Barcodes module.
 */

use Drupal\Core\Hook\Attribute\LegacyHook;
use Drupal\Core\Routing\RouteMatchInterface;
use Drupal\barcodes\Hook\BarcodesHelpHooks;
use Drupal\barcodes\Hook\BarcodesRenderHooks;

/**
 * Implements hook_help().
 */
#[LegacyHook]
function barcodes_help($route_name, RouteMatchInterface $route_match) {
  return \Drupal::service(BarcodesHelpHooks::class)->help($route_name, $route_match);
}

/**
 * Implements hook_theme().
 */
#[LegacyHook]
function barcodes_theme($existing, $type, $theme, $path) {
  return \Drupal::service(BarcodesRenderHooks::class)->theme($existing, $type, $theme, $path);
}
